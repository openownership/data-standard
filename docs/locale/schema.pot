msgstr ""

#. /properties/identifiers/title
#: schema/entity-statement.json:1 schema/person-statement.json:1
msgid "Identifiers"
msgstr ""

#. /properties/identifiers/description
#: schema/entity-statement.json:1
msgid ""
"One or more official identifiers for this entity. Where available, "
"official registration numbers should be provided."
msgstr ""

#. /properties/foundingDate/title
#: schema/entity-statement.json:1
msgid "Founding date"
msgstr ""

#. /properties/foundingDate/description
#: schema/entity-statement.json:1
