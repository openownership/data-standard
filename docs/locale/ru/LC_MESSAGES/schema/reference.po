msgid "direct"
msgstr "direct"

# 1650a83223414668be21633e1e489f40
#: ../../_build_schema/codelists/interestLevel.csv:1
msgid "Direct"
msgstr "Прямая"

# 1d20542acab040899d0aa482f550f7dc
#: ../../_build_schema/codelists/interestLevel.csv:1
