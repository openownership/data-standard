msgstr "NameType (Тип ФИО)"

# b2d0a74de32d43acb24160f0853c1862
#: ../../_build_schema/codelists/nameType.csv:1
msgid "individual"
msgstr "individual"

# 013938fa12b74a61bf898e2b79532edd
